set sandbox none
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
set selection-clipboard clipboard
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print
map g goto top

set default-bg                     "#0A0C08"
set default-fg                     "#FAFBEB"
set statusbar-bg                   "#0A0C08"
set statusbar-fg                   "#FAFBEB"
set inputbar-bg                    "#0A0C08"
set inputbar-fg                    "#FAFBEB"
set notification-bg                "#0A0C08"
set notification-fg                "#FAFBEB"
set notification-error-bg          "#0A0C08"
set notification-error-fg          "#FAFBEB"
set notification-warning-bg        "#0A0C08"
set notification-warning-fg        "#FAFBEB"
set highlight-color                "#8A8A6B"
set highlight-active-color         "#969E97"
set completion-bg                  "#8A8A6B"
set completion-fg                  "#969E97"
set completion-highlight-bg        "#8A8A6B"
set completion-highlight-fg        "#969E97"
set recolor-lightcolor             "#0A0C08"
set recolor-darkcolor              "#FAFBEB"
