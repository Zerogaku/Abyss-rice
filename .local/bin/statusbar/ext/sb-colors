# BATTERY - style 2
Baticon="^B9^^c#FFFFFF^" #combine them both
Batinfo="^b#3C3C3C^^c#FFFFFF^"

# DATE/TIME - style 1 (for both)
Calmod="^B5^^c#FFFFFF^"

Timemod="^B8^^c#000000^"

# internet - style 1
Intmod="^B1^^c#FFFFFF^"

# music - style 2
Musicon="^B2^^c#FFFFFF^"
